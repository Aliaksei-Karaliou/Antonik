	image[13] = '468x60_s13.gif';
	alt[13] = 'Блог гродненских новостей';
	url[13] = 'http://s13.ru/';
	type[13] = 'gif';

	var total_images = 14;
	var i = top_rand_number(total_images);

	if (type[i] != 'flash') 
	{
		document.write ('<a href="'+url[i]+'" target="_blank"><img src="http://forum.grodno.net/Images/banners/'+image[i]+'" border="0" alt="'+alt[i]+'"></a>')
	} 
	else 
	{
		document.write (image[i])
	}

document.write('');
}

function top_rand_number(n)
{
	var x;
	x=Math.round(Math.random()*100);
	x%=n;
	return x;
}
